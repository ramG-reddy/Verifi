generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SebiRegistry {
  reg_no        String @id
  entity_name   String
  category      String?
  contact_email String?
  contact_phone String?
  valid_from    DateTime?
  valid_to      DateTime?
  raw           Json?
  updated_at    DateTime @updatedAt
}
